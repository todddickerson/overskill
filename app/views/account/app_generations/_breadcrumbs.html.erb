<% app_generation ||= @app_generation %>
<% app ||= @app || app_generation&.app %>
<%= render 'account/apps/breadcrumbs', app: app %>
<%= render 'account/shared/breadcrumb', label: t('.label'), url: [:account, app, :app_generations] %>
<% if app_generation&.persisted? %>
  <%= render 'account/shared/breadcrumb', label: app_generation.label_string, url: [:account, app_generation] %>
<% end %>
<%= render 'account/shared/breadcrumbs/actions', only_for: 'app_generations' %>
