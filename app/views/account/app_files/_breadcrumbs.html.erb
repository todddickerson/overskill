<% app_file ||= @app_file %>
<% team ||= @team || app_file&.team %>
<%= render 'account/teams/breadcrumbs', team: team %>
<%= render 'account/shared/breadcrumb', label: t('.label'), url: [:account, team, :app_files] %>
<% if app_file&.persisted? %>
  <%= render 'account/shared/breadcrumb', label: app_file.label_string, url: [:account, app_file] %>
<% end %>
<%= render 'account/shared/breadcrumbs/actions', only_for: 'app_files' %>
