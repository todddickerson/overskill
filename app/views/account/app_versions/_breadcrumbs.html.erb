<% app_version ||= @app_version %>
<% app ||= @app || app_version&.app %>
<%= render 'account/apps/breadcrumbs', app: app %>
<%= render 'account/shared/breadcrumb', label: t('.label'), url: [:account, app, :app_versions] %>
<% if app_version&.persisted? %>
  <%= render 'account/shared/breadcrumb', label: app_version.label_string, url: [:account, app_version] %>
<% end %>
<%= render 'account/shared/breadcrumbs/actions', only_for: 'app_versions' %>
